<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<package format="1" xmlns="https://wiki.freecad.org/Package_Metadata">

  <name>Titleblock Workbench</name>

  <description>An extension for the TechDraw workbench to fill a TitleBlock with the aid of the Spreadsheet workbench.</description>

  <version>0.1.0</version>

  <date>2023-10-08</date>

  <maintainer email="paul.ebbers@gmail.com">Paul Ebbers</maintainer>

  <license file="LICENSE">LGPL-2.1-or-later</license>

  <pythonmin>3.0.0</pythonmin>

  <url branch="main" type="repository">https://github.com/APEbbers/TechDrawTitleBlockUtility</url>

  <url type="readme">https://github.com/APEbbers/TechDrawTitleBlockUtility/README.md</url>

  <icon>Resources/icons/TitleBlockWB.svg</icon>

  <content>
    <workbench>
      <classname>TitleBlockWB</classname>
      <subdirectory>./</subdirectory>
    </workbench>
    <workbench>
      <depend optional="False" type="automatic">Spreadsheet</depend>
      <depend optional="False" type="automatic">TechDraw</depend>
      <classname>TitleBlockWB</classname>
      <subdirectory>./.</subdirectory>
    </workbench>
  </content>

</package>
